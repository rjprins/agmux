    "chokidar": "^5.0.0",
    "fastify": "^5.7.4",
    "node-pty": "^1.1.0",
    "strip-ansi": "^7.1.2",
    "ws": "^8.19.0",
    "@xterm/xterm": "^5.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.3",
    "@types/ws": "^8.18.1",
    "esbuild": "^0.27.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
