{
  "name": "agent-tide",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "app": "node scripts/build-ui.mjs && node --import tsx src/server.ts",
    "app:watch": "node scripts/build-ui.mjs && node --watch --import tsx src/server.ts",
    "live": "node scripts/live.mjs",
    "dev": "npm run -s live",
    "ui:build": "node scripts/build-ui.mjs",
    "build": "tsc -p tsconfig.json && node scripts/build-ui.mjs",
    "start": "node dist/server.js",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "PWDEBUG=1 playwright test --headed",
    "test": "vitest run"
  },
